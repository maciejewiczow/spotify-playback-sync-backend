# spotify app params
SPOTIFY_APP_ID= <application id as registered in spotify>
SPOTIFY_APP_SECRET= <appolication secret from spoti>
SPOTIFY_REDIRECT_URL= <ulr to whitch spotify will redirect from their auth page, ie. http://localhost/callback>

# session setup
SESSION_SECRET= <secret string used to sign sessionID cookie>
SESSION_COOKIE_KEY= <name of the cookie used to store session ID on client>
AUTH_JWT_SECRET= <string used to sign tokens sent to clients>

# redis database credentials
REDIS_HOST= <addres of running redis server to connect to>
REDIS_PORT= <redis server port>
REDIS_PASS= <redis password>

# server setup
PORT= <port on whitch the server will listen for requests>